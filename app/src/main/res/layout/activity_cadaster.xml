<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:alpha="0.7">

    <ImageView
        android:id="@+id/return_home"
        android:layout_width="45dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/icone_fleche_gauche_orange"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp"
        tools:ignore="MissingConstraints"
        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Novo Cadastro"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="28dp" tools:ignore="MissingConstraints"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Confira se seus dados estão digitados corretamente"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="108dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/alert_icon"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="107dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Informações pessoais"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="201dp"
        tools:ignore="MissingConstraints" />

    <androidx.constraintlayout.widget.Barrier
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:scrollbarSize="50dp"
        app:barrierDirection="top" />

    <EditText
        android:id="@+id/textView6"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@drawable/insert_data"
        android:hint="Digite seu nome completo"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="237dp" />

    <EditText
        android:id="@+id/textView7"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@drawable/insert_data"
        android:hint="Digite seu CPF"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="303dp" />

    <EditText
        android:id="@+id/textView8"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@drawable/insert_data"
        android:hint="Digite seu email"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="370dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Senha"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="468dp" />

    <EditText
        android:id="@+id/textView10"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@drawable/insert_data"
        android:hint="Digite sua nova senha"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="497dp" />

    <EditText
        android:id="@+id/textView11"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@drawable/insert_data"
        android:hint="Digite sua senha novamente"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="564dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_finish_cadaster"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@drawable/background_button"
        android:text="@string/finish"
        android:textColor="@color/white"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="647dp" />

</androidx.constraintlayout.widget.ConstraintLayout>